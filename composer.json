{
    "name": "karpack/contracts",
    "description": "Contracts fulfilled by Karpack packages",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/karpack/contracts/issues",
        "source": "https://github.com/karpack/contracts"
    },
    "keywords": ["laravel", "contracts", "packages"],
    "authors": [
        {
            "name": "Kalesh Kaladharan",
            "email": "kalesh.13@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/contracts": ">=7.0",
        "illuminate/database": ">=7.0",
        "illuminate/support": ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "Karpack\\Contracts\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
